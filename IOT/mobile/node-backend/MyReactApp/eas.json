{
    "cli": {
      "version": ">= 3.13.3",
      "appVersionSource": "remote"
    },
    "build": {
      "preview": {
        "android": {
          "buildType": "apk",
          "gradleCommand": ":app:assembleRelease",
          "withoutCredentials": true
        },
        "extends": "production",
        "distribution": "internal",
        "env": {
          "APP_ENV": "preview"
        }
      },
      "production": {
        "projectRoot": "."
      }
    }
  }